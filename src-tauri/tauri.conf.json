{
  "productName": "Jedi",
  "version": "0.0.1",
  "identifier": "aaron.che",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:31420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "decorations": true,
        "resizable": true,
        "title": "Jedi",
        "width": 900,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["config/*"]
  }
}
